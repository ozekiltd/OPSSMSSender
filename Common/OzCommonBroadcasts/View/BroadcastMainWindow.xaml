                            <Rectangle Height="30" Width="1" Fill="#8fbd8f"/>
                            <Button Content="" Height="47" Style="{DynamicResource save_icon}" Width="47" Command="{Binding Save}"/>
                            <Rectangle Height="30" Width="1" Fill="#3d6a3b"/>
                            <Rectangle Height="30" Width="1" Fill="#8fbd8f"/>
                            <Button Content="" Height="47" Style="{DynamicResource start_icon}" Width="47" Command="{Binding Start}"/>
                            <Rectangle Height="30" Width="1" Fill="#3d6a3b"/>
                            <Rectangle Height="30" Width="1" Fill="#8fbd8f"/>
                            <Button Content="" Height="47" Style="{DynamicResource stop_icon}" Width="47" Command="{Binding Stop}"/>
