                        <Label Content="{Binding ApplicationInformation.ProductName}" DockPanel.Dock="Left" VerticalContentAlignment="Center" FontFamily="Arial" FontWeight="Normal" FontSize="20" Foreground="#bbc4bc"/>
                    </DockPanel>
                </DockPanel>

                <Grid Height="25" DockPanel.Dock="Bottom">
                    <Grid.Background>
                        <LinearGradientBrush EndPoint="0.5,1" StartPoint="0.5,0">
