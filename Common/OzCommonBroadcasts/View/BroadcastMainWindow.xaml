                        <TextBlock Visibility="Hidden"></TextBlock>
                    </DockPanel>
                </Grid>

                <DataGrid Margin="5" x:Name="CustumerDataGrid" ItemsSource="{Binding Customers, NotifyOnValidationError=True}" SelectionUnit="FullRow" ClipboardCopyMode="IncludeHeader"
                          AutoGenerateColumns="True" DockPanel.Dock="Bottom" SelectionMode="Extended" CanUserAddRows="False" ColumnWidth="*" 
                          KeyDown="DataGrid_KeyDown" CanUserSortColumns="False" IsReadOnly="{Binding IsReadOnly}"
                          AutoGeneratingColumn="CustumerDataGrid_AutoGeneratingColumn" 
