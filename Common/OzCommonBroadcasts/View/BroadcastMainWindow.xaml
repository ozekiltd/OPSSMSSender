                            <TextBlock Text="Overall progress" Foreground="White"/>
                            <ProgressBar Height="20"  Value="{Binding DoneJobs}" Maximum="{Binding AllJobs}" Width="200" Margin="10,0,10,0"/>
                            <TextBlock x:Name="TbckDoneMessage" Text="{Binding DoneJobs}" Foreground="White"/>
                            <TextBlock Text="/" Foreground="White"/>
