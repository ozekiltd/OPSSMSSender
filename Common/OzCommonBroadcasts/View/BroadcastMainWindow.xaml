                            <MenuItem x:Name="DatagridContext_CopyMenu" Header="_Copy" Command="{Binding Copy}" CommandParameter="{Binding RelativeSource={RelativeSource FindAncestor, AncestorType={x:Type ContextMenu}}, Path=PlacementTarget.SelectedItems}"/>
                            <MenuItem x:Name="DatagridContext_PasteMenu" Header="_Paste" Command="{Binding Paste}" CommandParameter="{Binding RelativeSource={RelativeSource FindAncestor, AncestorType={x:Type ContextMenu}}, Path=PlacementTarget.SelectedItems}"/>
                        </ContextMenu>
                    </DataGrid.ContextMenu>
                </DataGrid>

            </DockPanel>
            <tb:TaskbarIcon x:Name="TaskBarIcon" MenuActivation="RightClick" Style="{DynamicResource AppTaskbarIconStyle}" TrayLeftMouseUp="TaskBarIcon_OnTrayLeftMouseUp">
