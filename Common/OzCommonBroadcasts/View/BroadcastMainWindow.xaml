                            <GradientStop Color="#FF1A2F21" Offset="0"/>
                            <GradientStop Color="#FF20442A" Offset="1"/>
                        </LinearGradientBrush>
                    </Menu.Background>

                    <MenuItem Header="_File" Foreground="White" Style="{StaticResource menutitles}" Margin="10,0,5,0">
                        <MenuItem Header="_New..." Command="{Binding New}" Style="{StaticResource submenutitles}"/>
                        <Separator />
                        <MenuItem Header="_Open..." Command="{Binding Open}" Style="{StaticResource submenutitles}"/>
                        <Separator />
                        <MenuItem Header="_Save As..." Command="{Binding Save}" Style="{StaticResource submenutitles}"/>
