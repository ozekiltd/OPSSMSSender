                        <TextBlock x:Name="CloseButton" Text="r" FontFamily="Webdings" Foreground="Gray" Margin="5,0,8,0" HorizontalAlignment="Right" VerticalAlignment="Top" MouseLeftButtonUp="CloseButtonMouseLeftButtonUp" MouseEnter="CloseButton_OnMouseEnter" MouseLeave="CloseButton_MouseLeave"/>
                    </DockPanel>


