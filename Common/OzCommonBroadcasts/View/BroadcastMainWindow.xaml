                                            <MenuItem.Icon>
                                                <TextBlock Text="r" FontFamily="Webdings" Foreground="Gray" FontWeight="Bold"/>
                                            </MenuItem.Icon>
                                        </MenuItem>
                                    </ContextMenu>
                                </Button.ContextMenu>
                            </Button>
                            <Image  Source="..\View\Resources\ozeki_logo.png" Height="16" RenderOptions.BitmapScalingMode="HighQuality"/>
                            <Label Content="{Binding ApplicationInformation.ProductName}" VerticalContentAlignment="Center" FontFamily="Arial" FontSize="14" FontWeight="Normal" Foreground="#bbc4bc" DockPanel.Dock="Left"/>
                        </DockPanel>
                        <TextBlock DockPanel.Dock="Left" VerticalAlignment="Stretch" />
                    </DockPanel>

